package 제어문.순차문;


import java.util.Scanner;

import javax.swing.JOptionPane;

public class Scanner연습 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("나이 입력 >>");
		int age = sc.nextInt();
		
		System.out.println("당신의 나이는 >>"+age);
		
		
		
		
	}

}
