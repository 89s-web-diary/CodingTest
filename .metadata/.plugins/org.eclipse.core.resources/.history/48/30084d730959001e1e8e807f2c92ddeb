package 제어문.반복문;

public class 반복문확인문제1 {

	public static void main(String[] args) {

		while (true) {
			for (int i = 0; i < 10; i++) {
				System.out.print("*");
				break;
			}
		}
		System.out.println(" ");
		
		while (true) {
			for (int i = 0; i < 5; i++;) {
				System.out.print("커피*");
				break;
			}
		}
	}

}
